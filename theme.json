{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": false
				}
			},
			"core/group": {
				"color": {
					"background": true
				}
			},
			"core/pullquote": {
				"border": {
					"color": false,
					"radius": false,
					"style": false,
					"width": false
				}
			}
		},
		"color": {
			"custom": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"customDuotone": false,
			"customGradient": false,
			"gradients": [],
			"link": false,
			"palette": [
				{"color": "#000000",		"name": "Foreground",	"slug": "foreground"},
				{"color": "#ffffff",		"name": "Background",	"slug": "background"},
				{"color": "#F73F3B",		"name": "Red",			"slug": "red"},
				{"color": "#FF5E35",		"name": "Orange",		"slug": "orange"},
				{"color": "#FFA490",		"name": "Coral",		"slug": "coral"},
				{"color": "#FFC7BA",		"name": "Peach",		"slug": "peach"},
				{"color": "#452220",		"name": "Brown",		"slug": "brown"},
				{"color": "#C1A097",		"name": "Taupe",		"slug": "taupe"},
				{"color": "#E2C8C1",		"name": "Beige",		"slug": "beige"},
				{"color": "#5E1742",		"name": "Plum",			"slug": "plum"},
				{"color": "#E8B6CE",		"name": "Pink",			"slug": "pink"},
				{"color": "#C2D8E5",		"name": "Light blue",	"slug": "light-blue"},
				{"color": "#61AA99",		"name": "Turquoise",	"slug": "turquoise"},
				{"color": "#B6DAD2",		"name": "Mint",			"slug": "mint"},
				{"color": "#F4E365",		"name": "Lemon",		"slug": "lemon"},
				{"color": "#FBF3BD",		"name": "Light yellow",	"slug": "light-yellow"},

				{"color": "#4B4B4B",		"name": "Dark gray",	"slug": "dark-gray"},
				{"color": "#DBDBDB",		"name": "Medium gray",	"slug": "medium-gray"},
				{"color": "#F5F5F5",		"name": "Light gray",	"slug": "light-gray"}
			]
		},
		"custom": {
			"border-radius": {
				"default": "0.3125rem",
				"small": "0.625rem",
				"medium": "1.25rem",
				"large": "1.875rem"
			},
			"border-width": {
				"default": "1px"
			},
			"box-shadow": {
				"default": "0px 2px 8px rgba(0, 0, 0, 0.15)"
			},
			"color": {
				"link" : "#0000EE"
			},
			"layout": {
				"padding": "1.25rem",
				"content": "100.5rem",
				"wide": "110rem",
				"page": "var(--wp--custom--layout--content)",
				"block-gap": "calc(var(--wp--custom--layout--padding) * 2)",
				"block-gap-large": "calc(var(--wp--custom--layout--padding) * 4)"
			},
			"letter-spacing": {
				"none": "normal"
			},
			"line-height": {
				"default": 1.2
			}
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"position": {
			"sticky": false
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"letterSpacing": false,
			"lineHeight": false,
			"fontFamilies": [
				{
					"fontFamily": "\"GT America\", sans-serif",
					"name": "GT America",
					"slug": "gt-america",
					"fontFace": [
						{
							"fontFamily": "GT America",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/GT America/GT-America-Light.otf" ]
						},
						{
							"fontFamily": "GT America",
							"fontWeight": "300",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/GT America/GT America Light Italic.otf" ]
						},
						{
							"fontFamily": "GT America",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/GT America/GT America Regular.otf" ]
						},
						{
							"fontFamily": "GT America",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/GT America/GT America Regular Italic.otf" ]
						},
						{
							"fontFamily": "GT America",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/GT America/GT America Medium.otf" ]
						},
						{
							"fontFamily": "GT America",
							"fontWeight": "500",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/GT America/GT America Medium Italic.otf" ]
						},
						{
							"fontFamily": "GT America",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/GT America/GT America Bold.otf" ]
						}
					]
				},
				{
					"fontFamily": "\"GT America Condensed\", sans-serif",
					"name": "GT America Condensed",
					"slug": "gt-america-condensed",
					"fontFace": [
						{
							"fontFamily": "GT America Condensed",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/GT America Condensed/GT America Condensed Regular.otf" ]
						},
						{
							"fontFamily": "GT America Condensed",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/GT America Condensed/GT America Condensed Medium.otf" ]
						}
					]
				}
			],
			"fontSizes": [
				{"name": "Small",			"size": "0.75rem",							"slug": "small"},
				{"name": "Base",			"size": "1rem",								"slug": "base"},
				{"name": "Medium",			"size": "1.125rem",							"slug": "medium"},
				{"name": "Large",			"size": "1.5rem",							"slug": "large"},
				{"name": "Heading 1",		"size": "clamp(3.5rem, 5.6vw, 5.625rem)",	"slug": "heading-1"},
				{"name": "Heading 2",		"size": "clamp(3rem, 4.664vw, 4.6875rem)",	"slug": "heading-2"},
				{"name": "Heading 3",		"size": "clamp(2.25rem, 2.86vw, 2.875rem)",	"slug": "heading-3"},
				{"name": "Heading 4",		"size": "clamp(2rem, 2.239vw, 2.25rem)",	"slug": "heading-4"},
				{"name": "Heading 5",		"size": "clamp(1.75rem, 1.99vw, 2rem)",		"slug": "heading-5"},
				{"name": "Heading 6",		"size": "1.125rem",							"slug": "heading-6"}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"radius": "var(--wp--custom--border-radius--large)",
							"style": "solid",
							"width": "2px"
						},
						"spacing": {
							"padding": {
								"top": "0.3125rem",
								"right": "1.5rem",
								"bottom": "0.3125rem",
								"left": "1.5rem"
							}
						}
					}
				}
			},
			"core/group": {
				"spacing": {
					"padding": {
						"bottom": "calc(var(--wp--custom--layout--padding) * 2)",
						"left": "calc(var(--wp--custom--layout--padding) * 2)",
						"right": "calc(var(--wp--custom--layout--padding) * 2)",
						"top": "calc(var(--wp--custom--layout--padding) * 2)"
					}
				}
			},
			"core/image": {
				"variations": {
					"rounded": {
						"border": {
							"radius": "var(--wp--custom--border-radius--large)"
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "0"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--gt-america)",
							"lineHeight": "var(--wp--custom--line-height--default)",
							"fontSize": "var(--wp--preset--font-size--large)",
							"fontWeight": "400",
							"textTransform": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--layout--block-gap)",
						"left": "0",
						"right": "0",
						"top": "0"
					},
					"padding": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gt-america)",
					"lineHeight": "var(--wp--custom--line-height--default)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "300"
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--plum)",
					"text": "var(--wp--preset--color--plum)"
				},
				"spacing": {
					"margin": {
						"bottom": "calc(var(--wp--custom--layout--padding) * 2)",
						"top": "calc(var(--wp--custom--layout--padding) * 2)"
					}
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"bottom": "0",
						"top": "0"
					}
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--plum)",
					"width": "0 0 0 3px"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--gt-america)",
							"lineHeight": "var(--wp--custom--line-height--default)",
							"fontSize": "var(--wp--preset--font-size--base)",
							"fontWeight": "400",
							"textTransform": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--layout--block-gap)",
						"left": "0",
						"right": "0",
						"top": "0"
					},
					"padding": {
						"bottom": "0",
						"left": "calc(var(--wp--custom--layout--padding) * 1.5)",
						"right": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gt-america)",
					"lineHeight": "var(--wp--custom--line-height--default)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "400"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gt-america)",
					"lineHeight": "var(--wp--custom--line-height--default)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic",
					"fontWeight": "normal"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0",
					"width": "0"
				},
				"color": {
					"background": "transparent",
					"text": "inherit"
				},
				"spacing": {
					"padding": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gt-america)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--default)"
				},
				":hover": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline"
					},
					"outline": {
						"color": "#000",
						"offset": "0",
						"style": "solid",
						"width": "2px"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontSize": "0.875rem",
					"lineHeight": "var(--wp--custom--line-height--default)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gt-america)",
					"lineHeight": "var(--wp--custom--line-height--default)"
				},
				"spacing": {
					"margin": {
						"bottom": "1.25rem"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"fontWeight": "500"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)",
					"fontWeight": "500"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)",
					"fontWeight": "500"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)",
					"fontWeight": "500"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)",
					"fontWeight": "400"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gt-america-condensed)",
					"fontSize": "var(--wp--preset--font-size--heading-6)",
					"fontWeight": "500",
					"textTransform": "uppercase"
				}
			},
			"link": {
				"color": {
					"text": "inherit"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"color": {
						"text": "var(--wp--custom--color--foreground)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--custom--color--foreground)"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--block-gap)",
			"padding": {
				"bottom": "0",
				"left": "var(--wp--custom--layout--padding)",
				"right": "var(--wp--custom--layout--padding)",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--gt-america)",
			"fontSize": "var(--wp--preset--font-size--large)",
			"fontWeight": "300",
			"lineHeight": "var(--wp--custom--line-height--default)"
		}
	}
}